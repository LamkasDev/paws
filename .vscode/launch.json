{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/pawsc/main.go",
      "buildFlags": "-tags 'paws64 pawsdebug'",
      "output": "build/paws64/pawsc",
      "cwd": "build/paws64"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/run/media/cute-foxgirls/M2/Code/go/paws/build/paws64",
      "program": "/run/media/cute-foxgirls/M2/Code/go/paws/build/paws64/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}